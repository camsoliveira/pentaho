    <compatible>N</compatible>
    <optimizationLevel />
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here
var holiday_name = null;
var is_holiday = false;
var is_fixed_holiday = false;
var is_movabel_holiday = false;

if(movable_holiday_name &amp;&amp; fixed_holiday_name){
	holiday_name = fixed_holiday_name + " " + movable_holiday_name;
	is_fixed_holiday = true;
	is_movabel_holiday = true;
	is_holiday = true;
} else if(fixed_holiday_name) {
	holiday_name = fixed_holiday_name
	is_fixed_holiday = true;
	is_holiday = true;
} else if(movable_holiday_name) {
	holiday_name = movable_holiday_name
	is_movabel_holiday = true;
	is_holiday = true;
}</jsScript_script>
      </jsScript>
